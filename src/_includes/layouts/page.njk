---
layout: layouts/base.njk
---

<article class="page-content">
  <header class="page-header">
    <h1>{{ title }}</h1>
    {% if description %}
      <p class="page-description">{{ description }}</p>
    {% endif %}
  </header>
  
  <div class="page-body">
    {{ content | safe }}
  </div>
</article>
